{
  "name": "tic-tac-toe-client",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "analyze": "sonar-scanner",
    "build": "react-scripts build",
    "clean": "rimraf ./.scannerwork ./reports ./coverage",
    "eject": "react-scripts eject",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:stylelint",
    "lint:eslint": "eslint \"**/*\" --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:stylelint": "stylelint . --fix",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --coverage --reporters=default --reporters=jest-junit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard"
    ],
    "plugins": [
      "stylelint-no-unsupported-browser-features",
      "stylelint-scss",
      "stylelint-react-native"
    ],
    "rules": {
      "at-rule-no-unknown": null,
      "plugin/no-unsupported-browser-features": [
        true,
        {
          "severity": "warning"
        }
      ],
      "scss/at-rule-no-unknown": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "not dead",
    "not IE 9-11",
    "not ie_mob 11",
    "not OperaMini all"
  ],
  "jest": {
    "coverageReporters": [
      "text",
      "lcovonly",
      "cobertura"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-lodash)"
    ]
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "uniqueOutputName": "true"
  },
  "dependencies": {
    "axios": "1.7.7",
    "bootstrap": "5.3.3",
    "jquery": "3.7.1",
    "react": "18.3.1",
    "react-app-polyfill": "3.0.0",
    "react-bootstrap": "2.10.5",
    "react-dom": "18.3.1",
    "recurrent-js": "1.7.4",
    "reinforce-js": "1.5.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.13",
    "@types/node": "22.7.4",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.7.0",
    "@typescript-eslint/parser": "8.7.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-compat": "6.0.1",
    "eslint-plugin-html": "8.1.2",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.1.6",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.10",
    "sass": "1.79.4",
    "prettier": "3.3.3",
    "react-scripts": "5.0.1",
    "rimraf": "6.0.1",
    "sonarqube-scanner": "4.2.3",
    "stylelint": "16.9.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-no-unsupported-browser-features": "8.0.1",
    "stylelint-react-native": "2.7.0",
    "stylelint-scss": "6.7.0",
    "typescript": "4.9.5"
  }
}
