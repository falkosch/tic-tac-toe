{
  "name": "tic-tac-toe-client",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "analyze": "sonar-scanner",
    "build": "react-scripts build",
    "clean": "rimraf ./.scannerwork ./reports ./coverage",
    "eject": "react-scripts eject",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:stylelint",
    "lint:eslint": "eslint \"**/*\" --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:stylelint": "stylelint . --fix",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --coverage --reporters=default --reporters=jest-junit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard"
    ],
    "plugins": [
      "stylelint-no-unsupported-browser-features",
      "stylelint-scss",
      "stylelint-react-native"
    ],
    "rules": {
      "at-rule-no-unknown": null,
      "plugin/no-unsupported-browser-features": [
        true,
        {
          "severity": "warning"
        }
      ],
      "scss/at-rule-no-unknown": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "not dead",
    "not IE 9-11",
    "not ie_mob 11",
    "not OperaMini all"
  ],
  "jest": {
    "coverageReporters": [
      "text",
      "lcovonly",
      "cobertura"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-lodash)"
    ]
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "uniqueOutputName": "true"
  },
  "dependencies": {
    "axios": "0.19.2",
    "bootstrap": "4.4.1",
    "jquery": "3.4.1",
    "react": "16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-bootstrap": "1.0.0-beta.16",
    "react-dom": "16.12.0",
    "recurrent-js": "1.7.4",
    "reinforce-js": "1.5.1"
  },
  "devDependencies": {
    "@types/jest": "25.1.2",
    "@types/node": "13.7.1",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-compat": "3.5.1",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-markdown": "1.0.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "4.2.3",
    "jest-junit": "10.0.0",
    "lint-staged": "10.0.7",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "react-scripts": "3.3.1",
    "rimraf": "3.0.2",
    "sonarqube-scanner": "^2.5.0",
    "stylelint": "13.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-no-unsupported-browser-features": "4.0.0",
    "stylelint-react-native": "2.1.1",
    "stylelint-scss": "3.14.2",
    "typescript": "3.7.5"
  }
}
